{
  "name": "gnosis",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "firebase": "^4.13.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.2.2",
    "react-scripts-ts": "2.15.1",
    "tachyons": "^4.9.1"
  },
  "scripts": {
    "start:client": "react-scripts-ts start",
    "start:server": "firebase serve --only functions,database",
    "build:client": "react-scripts-ts build",
    "build:server": "npm --prefix src/gnosis/server/functions run build",
    "lint:client": "tslint",
    "lint:server": "npm --prefix src/gnosis/server/functions run lint",
    "deploy:server": "firebase deploy --only functions,database",
    "deploy:client": "firebase deploy --only hosting",
    "start": "npm run start:client",
    "build": "npm run build:client",
    "lint": "npm run lint:client",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "^10.0.4",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "@types/react-router-dom": "^4.2.6",
    "firebase-tools": "^3.18.4",
    "prettier": "^1.12.1",
    "typescript": "^2.8.3"
  }
}
